{
  "configurations": [
    {
      "name": "Launch N2K Analyzer",
      "request": "launch",
      "runtimeArgs": ["run", "server"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "outFiles": [
        "${workspaceFolder}/public/*.(m|c|)js",
        "${workspaceFolder}/dist/*.(m|c|)js",
        "${workspaceFolder}/dist/streams/*.(m|c|)js",
        "/usr/local/src/ts-pgns/dist/*.(m|c|)js",
        "/usr/local/src/canboatjs/dist/*.(m|c|)js"
        //"${fileWorkspaceFolder}/dist/*.(m|c|)js"
      ],
      "type": "node",
      "env": {
        "DEBUG": "canboatjs:yddevice"
      }
      //"outputCapture": "std"
    }
  ]
}
